<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="css/main.css">
</head>
<body>
    <header> 
        <ul></ul>
        <button> <a href="/logout">logout</a></button>
        <button> <a href="/register">Register</a></button>
        <button> <a href="/check"> check</a></button>
    </header>
    <aside>
        <h1>List of Registered student</h1> 
        <div>
            <% items.forEach(function(student) { %>
                <img src="data:image/<%student.student_image.contentType%>;base64,<%=student.student_image.data.toString('base64')%>" alt="" width=160 height=160 srcset="">
            <h5> <%=student.first_name %></h5> 
            <h5> <%=student.last_name %></h5> 

            <p> Update Record</p>
            <% }) %>
        </div>
    </aside>
</body>
</html>